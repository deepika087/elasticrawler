{
  "crawl": {
    "_source": {
      "enabled": true,
      "compress": true
    },
    "_all": {
      "enabled": false
    },
    "dynamic": "strict",
    "properties": {
      "name": {
        "type": "string",
        "index": "not_analyzed",
        "fields": {
          "search": {
            "type": "string",
            "analyzer": "standard"
          }
        }
      },
      "creationdate": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss:SSS"
      },
      "startdate": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss:SSS"
      },
      "enddate": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss:SSS"
      },      
      "seeds": {
        "type": "string",
        "index": "not_analyzed",
        "fields": {
          "search": {
            "type": "string",
            "analyzer": "uax_url_email"
          }
        }
      }
    }
  }
}
